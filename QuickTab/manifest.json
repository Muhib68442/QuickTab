{
  "manifest_version": 3,
  "name": "QuickTab",
  "description": "Quick tab",
  "version": "1.0.1",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [],

  "icons": {
    "16": "res/logo/icon16.png",
    "48": "res/logo/icon48.png",
    "128": "res/logo/icon128.png"
  },

  "web_accessible_resources": [
    {
      "resources": [
        "res/js/*.js",
        "res/theme/*",
        "res/logo/*"
      ],
      "matches": ["<all_urls>"]
    }
  ]

}
